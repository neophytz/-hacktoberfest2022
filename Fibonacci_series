#include <iostream>
using namespace std;

int main() {
    int n, i, f1, f2, f3;
    cout << "Enter the number of terms: ";
    cin >> n;

    i = 1;
    f1 = 0;
    f2 = 1;
    cout <<f1 << " " << f2 <<" ";
    while(i < n)
    {
        f3 = f1 + f2;
        cout << f3 << " ";
        f1 = f2;
        f2 = f3;
        i++;
    }
    return 0;
}

   